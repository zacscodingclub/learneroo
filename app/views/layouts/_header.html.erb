<header class="navbar">
  <ul>
    <li><%= link_to "Home", root_path, class:"button" %></li>
    <li><%= link_to "Browse All Products", products_path, class:"button" %></li>
    <div class="u-pull-right">
      <li><%= link_to "About", about_path, class:"button" %></li>
      <li><%= link_to "Contact", contact_path, class:"button" %></li>
      <% if user_signed_in? %>
        <%= link_to "Logout", destroy_user_session_path, method: :delete, class: "button" %>
      <% else %>
        <%= link_to "Sign Up", new_user_registration_path, class:"button" %>
        <%= link_to "Login", new_user_session_path, class:"button" %>
      <% end %>
    </div>
  </ul>
</header>